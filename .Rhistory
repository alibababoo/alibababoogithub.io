melted_data <- melt(swiftdata[, selected_attr])
# Create box plot
ggplot(melted_data, aes(x = 1, y = value)) +
geom_boxplot() +
labs(x = "", y = selected_attr, title = paste("Boxplot of", selected_attr)) +
theme_minimal()
})
}
